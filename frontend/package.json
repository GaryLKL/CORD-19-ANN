{
  "name": "template-frontend-react-redux-mui",
  "version": "1.0.0",
  "description": "A template for quickly setting up a react/redux/Material UI frontend",
  "engines": {
    "node": "12.12.0",
    "npm": "6.11.3"
  },
  "keywords": [
    "Material UI",
    "React",
    "Redux"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "./node_modules/webpack/bin/webpack.js -p --progress && node server.js",
    "build": "npm run clean && webpack --mode production --progress",
    "clean": "rm -rf build && mkdir build",
    "dev": "webpack-dev-server --mode development --progress",
    "doc": "webpack-dev-server --mode development --progress --host 0.0.0.0",
    "dev:hot": "webpack-dev-server --mode development --progress --hot",
    "serve": "serve build",
    "setup": "touch .env && npm install && npm audit fix",
    "test": "jest"
  },
  "author": "Maher Atashfaraz",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@date-io/moment": "^1.3.9",
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.2",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "core-js": "^3.1.4",
    "d3": "^5.9.7",
    "dotenv-defaults": "^1.0.2",
    "dotenv-webpack": "^1.7.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "brotli-webpack-plugin": "^1.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "serve": "^11.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
